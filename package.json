{
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "vite",
        "serve": "vite --host",
        "build": "vite build",
        "build:production": "NODE_ENV=production npm run build",
        "build:analyze": "npm run build && npm run analyze",
        "watch": "vite build --watch",
        "prod": "npm run build",
        "analyze": "node scripts/performance-monitor.js bundle",
        "test:performance": "node scripts/performance-monitor.js all",
        "vitals": "node scripts/performance-monitor.js vitals",
        "performance:trend": "node scripts/performance-monitor.js trend",
        "optimize:images": "imagemin resources/images/**/*.{jpg,png,svg} --out-dir=public/optimized-images",
        "lint:css": "stylelint resources/css/**/*.css",
        "lint:js": "eslint resources/js/**/*.{js,vue} --fix",
        "lint": "npm run lint:js && npm run lint:css",
        "format": "prettier --write resources/js/**/*.{js,vue} resources/css/**/*.css",
        "precommit": "npm run lint:js && npm run lint:css",
        "health-check": "node scripts/health-check.js",
        "deploy:staging": "npm run build && echo 'Staging deployment would run here'",
        "deploy:production": "npm run build:production && echo 'Production deployment would run here'",
        "clean": "rm -rf node_modules/.vite && rm -rf public/build",
        "clean:install": "npm run clean && npm ci",
        "security:audit": "npm audit --audit-level moderate",
        "security:fix": "npm audit fix",
        "security:check": "npm audit --audit-level high --json",
        "security:report": "npm audit --audit-level moderate --json > security-audit.json",
        "validate:optimization": "node scripts/optimization-validator.js",
        "performance:check": "node scripts/optimization-validator.js",
        "test": "npm run test:unit && npm run test:feature",
        "test:unit": "php artisan test --testsuite=Unit",
        "test:feature": "php artisan test --testsuite=Feature",
        "test:browser": "php artisan dusk",
        "test:coverage": "php artisan test --coverage",
        "deps:check": "npm outdated",
        "deps:update": "npm update",
        "deps:audit": "npm audit && npm run security:check"
    },
    "devDependencies": {
        "@playwright/test": "^1.55.0",
        "@tailwindcss/forms": "^0.5.0",
        "@tailwindcss/typography": "^0.5.0",
        "@vitejs/plugin-vue": "^5.0.0",
        "@vue/eslint-config-prettier": "^10.2.0",
        "autoprefixer": "^10.4.0",
        "eslint": "^9.0.0",
        "eslint-plugin-vue": "^10.4.0",
        "laravel-vite-plugin": "^1.0.0",
        "postcss": "^8.4.31",
        "postcss-import": "^16.0.0",
        "prettier": "^3.0.0",
        "stylelint": "^16.0.0",
        "stylelint-config-standard": "^36.0.0",
        "tailwindcss": "^3.4.0",
        "tailwindcss-dir": "4.0.0",
        "terser": "^5.43.1",
        "vite": "^5.4.0",
        "vue": "^3.5.0"
    },
    "dependencies": {
        "@chenfengyuan/vue-countdown": "^2.1.2",
        "@inertiajs/vue3": "^2.0.0",
        "@primeuix/themes": "^1.0.0",
        "@tailwindcss/aspect-ratio": "^0.4.0",
        "@tailwindcss/container-queries": "^0.1.1",
        "@tiptap/extension-color": "^2.8.0",
        "@tiptap/extension-highlight": "^2.8.0",
        "@tiptap/extension-image": "^2.8.0",
        "@tiptap/extension-link": "^2.8.0",
        "@tiptap/extension-mathematics": "^2.8.0",
        "@tiptap/extension-placeholder": "^2.8.0",
        "@tiptap/extension-subscript": "^2.8.0",
        "@tiptap/extension-superscript": "^2.8.0",
        "@tiptap/extension-text-align": "^2.8.0",
        "@tiptap/extension-text-style": "^2.8.0",
        "@tiptap/extension-underline": "^2.8.0",
        "@tiptap/starter-kit": "^2.8.0",
        "@tiptap/vue-3": "^2.8.0",
        "@vuelidate/core": "2.0.3",
        "@vuelidate/validators": "2.0.4",
        "@vueuse/core": "^11.1.0",
        "@vueuse/head": "2.0.0",
        "axios": "^1.6.8",
        "chart.js": "4.4.0",
        "dompurify": "^3.2.6",
        "katex": "^0.16.0",
        "lodash": "4.17.21",
        "mitt": "3.0.1",
        "nprogress": "0.2.0",
        "pinia": "2.1.7",
        "plyr": "3.7.8",
        "primeicons": "^7.0.0",
        "primevue": "^4.3.6",
        "sweetalert2": "^11.22.4",
        "swiper": "11.1.14",
        "tiptap-text-direction-extension": "^1.0.7",
        "v-calendar": "3.1.2",
        "vue-chartjs": "5.3.1",

        "vue-plyr": "7.0.0",
        "vue-select": "4.0.0-beta.6",
        "vue-sweetalert2": "5.0.11",
        "vuedraggable": "4.1.0",
        "ziggy-js": "^2.5.3"
    },
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=9.0.0"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not dead",
        "not ie 11"
    ]
}
