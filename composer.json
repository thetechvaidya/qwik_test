{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "barryvdh/laravel-dompdf": "^3.1",
        "bavix/laravel-wallet": "^11.4",
        "cviebrock/eloquent-sluggable": "^11.0",
        "doctrine/dbal": "^4.3",
        "guzzlehttp/guzzle": "^7.8",
        "inertiajs/inertia-laravel": "^2.0",
        "laravel/framework": "^11.0",
        "laravel/jetstream": "^5.0",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.7",
        "league/flysystem-aws-s3-v3": "^3.10",
        "maatwebsite/excel": "^3.1",
        "paquettg/php-html-parser": "^3.1",
        "razorpay/razorpay": "^2.9",
        "spatie/laravel-fractal": "^6.3",
        "spatie/laravel-medialibrary": "^11.0",
        "spatie/laravel-permission": "^6.21",
        "spatie/laravel-schemaless-attributes": "^2.5",
        "spatie/laravel-settings": "^3.4",
        "staudenmeir/belongs-to-through": "^2.16",
        "staudenmeir/eloquent-has-many-deep": "^1.20",
        "stripe/stripe-php": "^7.128",
        "tightenco/ziggy": "^2.0",
        "vinkla/hashids": "^12.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.16",
        "spatie/laravel-ignition": "^2.9",
        "fakerphp/faker": "^1.23",
        "laravel/sail": "^1.0",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "phpunit/phpunit": "^11.0",
        "spatie/laravel-ray": "^1.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/GlobalHelper.php",
            "app/Helpers/QuestionHelper.php",
            "app/Helpers/TranslationHelper.php",
            "app/Helpers/PaymentHelper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
